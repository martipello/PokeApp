<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- UNSELECTED TAB STATE -->
    <item android:state_selected="false" android:state_pressed="false">
        <layer-list>
            <!-- Bottom indicator color for the UNSELECTED tab state -->
            <item>
                <shape android:shape="rectangle">
                    <corners
                        android:topLeftRadius="@dimen/qualified_medium_margin_16dp"
                        android:topRightRadius="@dimen/qualified_medium_margin_16dp"
                        android:bottomLeftRadius="@dimen/qualified_medium_margin_16dp"
                        android:bottomRightRadius="@dimen/qualified_medium_margin_16dp"/>
                    <solid android:color="#65acee"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>