<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/scroll_wheel"
            android:layout_width="@dimen/qualified_large_margin_32dp"
            android:layout_height="@dimen/qualified_large_margin_32dp"
            android:layout_marginTop="@dimen/qualified_large_margin_64dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/scroll_wheel"
            android:layout_width="@dimen/qualified_large_margin_32dp"
            android:layout_height="@dimen/qualified_large_margin_32dp"
            android:layout_marginBottom="@dimen/qualified_medium_margin_16dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:rotation="-720" />
    </ConstraintSet>
</MotionScene>